      "outputs": {
        "object": {
          "$ref": "#Test.Object",
          "$id": "#Test.object"
        }
      },
      "types": {
        "Object": {
          "type": "object",
          "description": "An object of arbitrary structure.",
          "$id": "#Test.Object"
        }
      },
