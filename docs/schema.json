    "FlightBooking": {
      "description": "Allows automating airplane tickets booking and collecting related information.",
      "inputs": {
        "url": {
          "$ref": "#Generic.URL",
          "$id": "#FlightBooking.url"
        },
        "flight": {
          "$ref": "#FlightBooking.Flight",
          "$id": "#FlightBooking.flight"
        },
        "account": {
          "$ref": "#Generic.Account",
          "$id": "#FlightBooking.account"
        },
        "passengers": {
          "$ref": "#FlightBooking.Passengers",
          "$id": "#FlightBooking.passengers"
        },
        "payment": {
          "$ref": "#Generic.Payment",
          "$id": "#FlightBooking.payment"
        },
        "selectedSeats": {
          "$ref": "#FlightBooking.SelectedSeats",
          "$id": "#FlightBooking.selectedSeats"
        },
        "finalPriceConsent": {
          "$ref": "#Generic.FinalPriceConsent",
          "$id": "#FlightBooking.finalPriceConsent"
        }
      },
      "outputs": {
        "availableSeats": {
          "$ref": "#FlightBooking.AvailableSeats",
          "$id": "#FlightBooking.availableSeats"
        },
        "finalPrice": {
          "$ref": "#Generic.FinalPrice",
          "$id": "#FlightBooking.finalPrice"
        },
        "bookingConfirmation": {
          "$ref": "#FlightBooking.BookingConfirmation",
          "$id": "#FlightBooking.bookingConfirmation"
        }
      },
      "types": {
        "AvailableSeats": {
          "description": "Seat selection metadata extracted from website.",
          "type": "array",
          "items": {
            "$ref": "#FlightBooking.AvailableSeat"
          },
          "additionalProperties": "false",
          "$id": "#FlightBooking.AvailableSeats"
        },
        "AvailableSeat": {
          "properties": {
            "seatId": {
              "type": "string",
              "description": "Seat identifier."
            },
            "price": {
              "$ref": "#Generic.Price",
              "description": "Price, if available."
            }
          },
          "additionalProperties": "false",
          "required": [
            "seatId",
            "price"
          ],
          "$id": "#FlightBooking.AvailableSeat"
        },
        "SelectedSeats": {
          "description": "Seats selected for each passenger. Must match identifiers from <code>Flight.availableSeats</code> output.",
          "properties": {
            "seatIds": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "description": "Must match <code>seatId</code> from <code>Flight.availableSeats</code>"
            }
          },
          "additionalProperties": "false",
          "required": [
            "seatIds"
          ],
          "$id": "#FlightBooking.SelectedSeats"
        },
        "BookingConfirmation": {
          "description": "Emitted on \"Booking success\" page.",
          "properties": {
            "bookingReference": {
              "type": "string",
              "description": "Booking reference grabbed from the page."
            },
            "message": {
              "type": "string",
              "description": "Message about successful booking."
            },
            "price": {
              "$ref": "#Generic.Price",
              "description": "Price, if found on the successful booking page."
            }
          },
          "additionalProperties": "false",
          "required": [
            "bookingReference"
          ],
          "$id": "#FlightBooking.BookingConfirmation"
        }
      },
      "$id": "#FlightBooking"
    },
    "Test": {
      "description": "Internal domain for testing platform features.",
      "inputs": {
