      "types": {
        "URL": {
          "type": "string",
          "format": "url",
          "title": "URL Record",
          "description": "URL Record, as defined by https://url.spec.whatwg.org/#concept-url",
          "$id": "#Generic.URL"
        },
        "FinalPrice": {
          "description": "Emitted immediately before placing order, when final price is available.",
          "properties": {
            "price": {
              "$ref": "#Generic.Price",
              "description": "Final price (including all surcharges) as displayed on \"Pay Now\" page."
            }
          },
          "required": [
            "price"
          ],
          "$id": "#Generic.FinalPrice"
        },
        "FinalPriceConsent": {
          "description": "Client's consent for final price. Must exactly match the <code>FinalPrice</code> object.",
          "properties": {
            "price": {
              "$ref": "#Generic.Price"
            }
          },
          "additionalProperties": "false",
          "required": [
            "price"
          ],
          "$id": "#Generic.FinalPriceConsent"
        },
        "Price": {
          "type": "object",
          "properties": {
            "currencyCode": {
              "$ref": "#Generic.CurrencyCode"
            },
            "value": {
              "type": "integer",
              "minimum": 0,
              "description": "A positive integer in the smallest currency unit (100 for Â£1.00)."
            }
          },
          "required": [
            "currencyCode",
            "value"
          ],
          "additionalProperties": false,
          "$id": "#Generic.Price"
        },
