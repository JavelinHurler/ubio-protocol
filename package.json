{
  "name": "@ubio/protocol",
  "version": "5.5.1",
  "description": "ubio Automation Protocol",
  "main": "out/main/index.js",
  "types": "out/main/index.d.ts",
  "author": "ubio",
  "license": "MIT",
  "files": [
    "src/",
    "out/"
  ],
  "browser": {
    "node-fetch": "./fetch-browser.js"
  },
  "scripts": {
    "clean": "rm -rf out",
    "check": "tslint --project ./tsconfig.json",
    "dev": "run-s clean dev:ts",
    "dev:ts": "tsc -w",
    "test": "NODE_ENV=test mocha --opts ./mocha.opts",
    "dev-site": "run-p watch:* serve",
    "build": "rimraf public && npm run build:copy-static && npm run build:js && npm run build:schema && npm run build:html-cache",
    "serve": "serve public",
    "build:copy-static": "node ./build-copy-static.js",
    "build:js": "browserify site/ -o public/app.js",
    "build:schema": "node ./build-schema.js",
    "build:html-cache": "node ./build-html-cache.js",
    "watch:js": "watchify -p browserify-hmr site/ -o public/app.js",
    "preversion": "run-s check test",
    "version": "run-s build",
    "postversion": "npm publish --access public && git push origin master --tags"
  },
  "pre-commit": [
    "check"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@types/chai": "^4.2.7",
    "@types/glob": "^7.1.1",
    "@types/jsonpointer": "^4.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.1",
    "@types/node-fetch": "^2.5.4",
    "babel-core": "^6.26.3",
    "babelify": "^10.0.0",
    "browserify-hmr": "^0.3.5",
    "chai": "^4.2.0",
    "eslint": "^6.4.0",
    "eslint-config-ub": "^3.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "glob": "^7.1.6",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.0",
    "serve": "^11.1.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "jsonpointer": "^4.0.1",
    "node-fetch": "^2.6.0"
  }
}
