{
  "name": "@ubio/protocol",
  "version": "0.0.1",
  "description": "ubio Automation Protocol",
  "main": "src/index.js",
  "author": "ubio",
  "license": "MIT",
  "scripts": {
    "check": "eslint --ext=js,vue .",
    "dev": "run-p watch:* serve",
    "build": "run-p build:* && git add docs/build",
    "serve": "ws -d docs",
    "build:js": "browserify dev/ -o docs/build/app.js",
    "build:schema": "node ./build-schema.js",
    "watch:js": "watchify -p browserify-hmr dev/ -o docs/build/app.js",
    "test": "mocha",
    "changelog": "node changelog.js",
    "preversion": "run-s check test build changelog",
    "version": "git add -A",
    "postversion": "npm publish --access public && git push origin master --tags"
  },
  "pre-commit": [
    "check",
    "build"
  ],
  "devDependencies": {
    "ajv": "^5.3.0",
    "babel-core": "^6.26.0",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "browserify-hmr": "^0.3.5",
    "eslint": "^4.10.0",
    "eslint-config-ub": "^2.0.0",
    "eslint-plugin-vue": "^2.1.0",
    "expect": "^21.2.1",
    "local-web-server": "^2.3.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "vue": "^2.5.3",
    "vue-router": "^3.0.1",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  }
}
